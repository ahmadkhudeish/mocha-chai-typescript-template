{
  "name": "mocha-tyepscript",
  "version": "1.2.29",
  "description": "Mocha Typescript Unit Testing Template",
  "scripts": {
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "/var/www/coverage",
    "sourceMap": true,
    "instrument": true
  },
  "author": "ahmadkhudeish.co.nz",
  "license": "MIT",
  "dependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^6.0.92",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "source-map-support": "^0.4.17",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2"
  }
}
